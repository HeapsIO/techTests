<css>
button {
    background : #333;
    padding : 5 20;
    text {
        color : @textColor;
    }
    &.disabled {
        text {
            color : black;
        }
    }
}
</css>

<params>
{
    text : "Hello World",
    classes : ["disabled"],
}
</params>

<button(text)>
    <text text={text}/>
</button>